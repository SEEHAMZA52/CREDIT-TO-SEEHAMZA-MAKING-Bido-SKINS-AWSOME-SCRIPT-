local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("BidoSkins V20", "BloodTheme")
local Tab = Window:NewTab("BidoSkins V20")
local Section = Tab:NewSection("infinite yield")
Section:NewButton("inf yield", "Button for infinite yield", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()

end)
local Tab = Window:NewTab("Hubs")
local Section = Tab:NewSection("Fly")
Section:NewButton("Fly Gui V3", "Makes You Fly", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/XNEOFF/FlyGuiV3/main/FlyGuiV3.txt"))()
end)
local Tab = Window:NewTab("Blade Ball Script")
local Section = Tab:NewSection("Auto Parry")
Section:NewButton("Blade Ball Script", "Makes You Auto Parry", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/1f0yt/community/main/Circle"))()
end)
local Tab = Window:NewTab("Gui Animation")
local Section = Tab:NewSection("Animation 2024")
Section:NewButton("Animation 2024", "Buttonlnfo", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/CITY512/scripts/main/freeanims.lua"))()
end)
